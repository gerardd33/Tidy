singleton class FunctionsAndActions { 
    
    functions: {
        someFunction: (f: get (x: Int) -> Int, g: get (x: Int) -> Bool) -> Bool = {
            if (local.g(5)) then local.f(5) else local.f(7)
        }
    }
    
    actions: {
        someAction: (f: do (x: Int, y: Int) -> Void, g: get (x: Int) -> Bool) -> Void = {
            if (local.g(5)) {
                Pass
            } else {
                local get f(3, 3)
            }
        }
    }
}
